{
  "info": {
    "name": "Manager User API - Lead Flow App",
    "description": "Coleção de endpoints para gerenciamento de usuários (Manager/Operator) no Lead Flow App. Inclui operações CRUD para criar, listar, associar, dissociar e remover usuários.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000",
      "type": "string",
      "description": "Base URL da aplicação"
    },
    {
      "key": "supabaseUserId",
      "value": "ed4ab5a4-3188-41fa-8389-481784cb1f84",
      "type": "string",
      "description": "ID do usuário autenticado no Supabase (Manager)"
    },
    {
      "key": "managerId",
      "value": "",
      "type": "string",
      "description": "ID do manager criado - será preenchido automaticamente"
    },
    {
      "key": "operatorId",
      "value": "",
      "type": "string",
      "description": "ID do operator criado - será preenchido automaticamente"
    }
  ],
  "item": [
    {
      "name": "1. Criar Manager",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Status code é 200', function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test('Response tem estrutura Output', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response).to.have.property('isValid');",
              "    pm.expect(response).to.have.property('successMessages');",
              "    pm.expect(response).to.have.property('errorMessages');",
              "    pm.expect(response).to.have.property('result');",
              "});",
              "",
              "pm.test('Manager criado com sucesso', function () {",
              "    const response = pm.response.json();",
              "    pm.expect(response.isValid).to.be.true;",
              "    pm.expect(response.result).to.have.property('id');",
              "    pm.expect(response.result.name).to.eql('Maria Silva Manager');",
              "    pm.expect(response.result.role).to.eql('manager');",
              "    ",
              "    // Salvar o ID do manager para usar em outros testes",
              "    if (response.result && response.result.id) {",
              "        pm.collectionVariables.set('managerId', response.result.id);",
              "    }",
              "});"
            ]
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "x-supabase-user-id",
            "value": "{{supabaseUserId}}",
            "description": "ID do usuário autenticado no Supabase (deve ser Manager)"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Maria Silva Manager\",\n  \"email\": \"maria.manager@leadflow.com\",\n  \"role\": \"manager\",\n  \"profileIconUrl\": \"https://avatar.vercel.sh/maria\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/manager/{{supabaseUserId}}/users",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "manager", "{{supabaseUserId}}", "users"]
        }
      }
    },
